<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="boostpython" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="api" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="../../bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="api" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="../../bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++14" />
			<Add option="-fexceptions" />
			<Add directory=".." />
			<Add directory="../../../dlib" />
			<Add directory="../.." />
			<Add directory="$(#python.include)" />
		</Compiler>
		<Linker>
			<Add option="-pthread" />
			<Add library="blas" />
			<Add library="lapack" />
			<Add library="shyftcore" />
			<Add library="boost_python3" />
			<Add library="python3.4m" />
			<Add directory="$(#python.lib)" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
